<?xml version="1.0" encoding="ISO-8859-1"?>
<IreneLoader>
  
  <Param>
    <name>fName</name>
    <dataType>string</dataType>
    <value>IreneLoader</value>
  </Param>

  <Param>
    <name>fDebug</name>
    <dataType>string</dataType>
    <value>INFO</value>
  </Param>

  <!-- Histogram with the number of particles per event -->
  <H1D>
    <name>fIreneLoader_NumParts_H1</name>
    <title>Number of Particles </title>
    <nbinsx>30</nbinsx>
    <xlow>0.0</xlow>
    <xup>30.0</xup>
  </H1D>
  
  <!-- Histogram with the number of true tracks per event -->
  <H1D>
    <name>fIreneLoader_NumTTracks_H1</name>
    <title>Number of True Tracks </title>
    <nbinsx>30</nbinsx>
    <xlow>0.0</xlow>
    <xup>30.0</xup>
  </H1D>
  
  <!-- Histogram of true tracks Energy -->
  <H1D>
    <name>fIreneLoader_TTrackEnergy_H1</name>
    <title>True Tracks Energy</title>
    <nbinsx>25</nbinsx>
    <xlow>0.0</xlow>
    <xup>2500.0</xup>
  </H1D>
  
</IreneLoader>
