<?xml version="1.0" encoding="ISO-8859-1"?>
<RoadFilter>
  
  <Param>
    <name>fName</name>
    <dataType>string</dataType>
    <value>RoadFilter</value>
  </Param>
  
  <!-- Detector size -->
  <Param>
    <name>fMinDetX</name>
    <dataType>double</dataType>
    <value>-550.</value>
  </Param>

  <Param>
    <name>fMaxDetX</name>
    <dataType>double</dataType>
    <value>550.</value>
  </Param>

  <Param>
    <name>fMinDetY</name>
    <dataType>double</dataType>
    <value>-550.</value>
  </Param>

  <Param>
    <name>fMaxDetY</name>
    <dataType>double</dataType>
    <value>550.</value>
  </Param>

  <Param>
    <name>fMinDetZ</name>
    <dataType>double</dataType>
    <value>-700.</value>
  </Param>

  <Param>
    <name>fMaxDetZ</name>
    <dataType>double</dataType>
    <value>700.</value>
  </Param>


  <!-- Road Width -->
  <Param>
    <name>fRoadWidth</name>
    <dataType>double</dataType>
    <value>30.</value>
  </Param>


  <!-- Histogram with number of roads -->
  <H1D>
    <name>fRoadFilter_NumRoads_H1</name>
    <title>Number of Roads</title>
    <nbinsx>10</nbinsx>
    <xlow>0.0</xlow>
    <xup>10.0</xup>
  </H1D>


</RoadFilter>
